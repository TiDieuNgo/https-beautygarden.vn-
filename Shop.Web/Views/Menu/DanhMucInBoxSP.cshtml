@model IList<Shop.Model.Menu>
<div class="col-categori">
    <ul>
        <!--chi hien thi danh muc level 2-->
        @if(Model.Any())
        {
            foreach (var menu in Model)
            {
                  <li><a href="/sanpham/danhsach/@menu.id_">@menu.NameProduct</a></li>
            }
        }
    </ul>
    <a class="btn-show-cat btn-cat" title="tất cả danh mục">Tất cả danh mục<i aria-hidden="true" class="fa fa-angle-double-right"></i></a>
</div>
